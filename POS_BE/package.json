{
  "name": "pos_be",
  "version": "1.0.0",
  "description": "POS Awalan.Coffee",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "nodemon -r dotenv/config server.js dotenv_config_path=./env/.env",
    "build:exe": "pkg . --targets node18-win-x64 --output POS_AWALAN.exe"
  },
  "author": "Romi Rahman",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.44.0",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "iconv-lite": "^0.7.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^3.15.3",
    "node-thermal-printer": "^4.5.0",
    "nodemon": "^3.1.10",
    "pdf-to-printer": "^5.6.1",
    "pdfkit": "^0.17.2",
    "pngjs": "^7.0.0",
    "socket.io": "^4.8.1",
    "usb": "^2.16.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "pkg": {
    "scripts": [
      "server.js"
    ],
    "assets": [
      "production/**/*",
      "public/**/*",
      "env/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "buildExe"
  }
}
